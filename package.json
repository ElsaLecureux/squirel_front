{
  "name": "squirel",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start -c",
    "android": "DARK_MODE=media expo start --android",
    "ios": "DARK_MODE=media expo start --ios",
    "web": "DARK_MODE=media expo start --web",
    "build": "npx expo export --platform web",
    "prebuild": "node -e \"const fs=require('fs'); if (fs.existsSync('.env')) fs.copyFileSync('.env', '.env.local');\"",
    "test": "jest",
    "lint": "eslint ."
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@fortawesome/react-native-fontawesome)"
    ]
  },
  "expo": {
    "web": {
      "bundler": "metro",
      "output": "static"
    },
     "doctor": {
    "reactNativeDirectoryCheck": {
      "exclude": [
        "@apollo/client",
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@tamagui/babel-plugin",
        "@tamagui/image",
        "@tamagui/lucide-icons",
        "@tamagui/tabs",
        "graphql",
        "react-native-vector-icons",
        "@fortawesome/fontawesome-svg-core",
        "@fortawesome/free-brands-svg-icons",
        "@fortawesome/free-regular-svg-icons",
        "@fortawesome/free-solid-svg-icons",
        "jwt-decode"
      ]
    }
  }
  },
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.13.6",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@expo-google-fonts/bubblegum-sans": "^0.3.0",
    "@expo-google-fonts/medievalsharp": "^0.2.3",
    "@expo-google-fonts/mystery-quest": "^0.3.0",
    "@expo/metro-runtime": "~4.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-native-fontawesome": "^0.3.2",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@tamagui/babel-plugin": "^1.112.12",
    "@tamagui/config": "^1.112.12",
    "@tamagui/image": "^1.112.12",
    "@tamagui/lucide-icons": "^1.112.12",
    "@tamagui/tabs": "^1.126.3",
    "axios": "^1.7.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "^52.0.11",
    "expo-font": "~13.0.1",
    "expo-navigation-bar": "~4.0.4",
    "expo-screen-orientation": "~8.0.0",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "graphql": "^16.10.0",
    "jwt-decode": "^4.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tamagui": "^1.112.12"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-dotenv-import": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "jscodeshift": "^0.15.2",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.3.3"
  },
  "private": true
}
